<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite verbose="1" name="ebay">

    <parameter name="suiteOwner" value="qpsdemo"/>
    <parameter name="jenkinsJobName" value="JobJobJob"/>
    <parameter name="jenkinsJobType" value="web"/>
    <parameter name="jenkinsEmail" value="demo@qaprosoft.com"/>
    <parameter name="jenkinsEnvironments" value="DEMO"/>
    <parameter name="provider" value="selenium"/>

    <test parallel="methods" thread-count="5" name="ebayTest">
        <classes>
            <class name="com.solvd.carinatest.EbayTest">
                <methods>
                    <include name="checkSearchTooltipTest"/>
                    <include name="checkSearchTest"/>
                    <include name="checkUnderPriceFilterTest"/>
                    <include name="checkFromToPriceFilterTest"/>
                    <include name="checkStorageCapacityFilter"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>